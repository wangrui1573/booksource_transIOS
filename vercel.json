{
    "version": 2,
    "builds": [
      {
        "src": "./api/(.*)",
        "use": "@vercel/python"
      }
    ],
    "routes": [
     {
          "src": "/json/(.*)",
          "dest": "/api/json/$1"
      },
      {
          "src": "/templates/(.*)",
          "dest": "/api/templates/$1"
      },
      {
          "src": "/(.*)",
          "dest": "/api/$1"
      }
    ]
}